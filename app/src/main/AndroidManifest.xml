<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bulletin_test">

    <uses-permission-sdk-23 android:name="android.permission.UPDATE_DEVICE_STATS" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bulletin_Test">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity"></activity>
        <activity android:name=".ui.login.SignUpActivity"></activity>
        <activity android:name=".ui.login.PasswordResetActivity"></activity>
        <activity android:name=".ui.login.MemberActivity"></activity>
        <activity android:name=".ui.login.CameraActivity"></activity>
        <activity android:name=".ui.gallery.galleryActivity"></activity>
        <activity android:name=".ui.community.communityActivity"></activity>
        <activity android:name=".ui.community.freeCommunityActivity"></activity>
        <activity android:name=".ui.community.recipeCommunityActivity"></activity>
        <activity android:name=".ui.writingContent.writingRecipePostActivity"></activity>
        <activity android:name=".ui.writingContent.writingFreePostActivity"></activity>

    </application>

</manifest>